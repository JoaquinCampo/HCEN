    <TestPlan guiclass="TestPlanGui" testclass="TestPlan" testname="Clinical History Stability Test" enabled="true">
      <stringProp name="TestPlan.comments">Prueba de estabilidad para consulta de historia cl√≠nica. Verifica que los tiempos de respuesta no se degraden a lo largo de la prueba.</stringProp>
      <boolProp name="TestPlan.functional_mode">false</boolProp>
      <boolProp name="TestPlan.tearDown_on_shutdown">true</boolProp>
      <boolProp name="TestPlan.serialize_threadgroups">false</boolProp>
